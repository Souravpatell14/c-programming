#include <stdio.h>

int main()
{
    for(int i=1;i<=100;i++){
    printf("%d ",i);
}
    return 0;
}

2]even number 
include <stdio.h>

int main()
{
    for(int i=2;i<=100;i=i+2){
    printf("%d ",i);
}
    return 0;
}




3] 19 tabel
#include <stdio.h>

int main()
{
    for(int i=1;i<=190;i=i+1){
   if(i%19==0) printf("%d\n",i);
}
    return 0;
}


(4) odd number 
#include <stdio.h>

int main()
{
    int n;
    printf("enter the number ");
    scanf("%d",&n);
    for(int i=1;i<=n*2-1;i=i+2){
    printf("%d\n",i);
}
    return 0;
}

(5) 3 ka difference 
#include <stdio.h>

int main()
{
    int n;
    printf("enter the number ");
    scanf("%d",&n);
    for(int i=1;i<=n*2+1;i=i+3){
    printf("%d\n",i);
}
    return 0;
}